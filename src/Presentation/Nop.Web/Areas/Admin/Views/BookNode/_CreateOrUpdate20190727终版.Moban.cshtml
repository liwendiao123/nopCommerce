@model AiBookModelView
@using Newtonsoft.Json;
@using Nop.Web.Models.Api.BookNode;
@{
    if (!string.IsNullOrEmpty(Model.UnityStrJson))
    {
        Model.BookNodeRoot = JsonConvert.DeserializeObject<BookNodeNewRoot>(Model.UnityStrJson);

        if (Model != null
            && Model.BookNodeRoot != null
            && Model.BookNodeRoot.Base != null
            )
        {
            if (Model.ResourceList == null)
            {
                Model.ResourceList = new List<SelectListItem>();
            }
            if (Model.BookNodeRoot.Base.buttoninfo != null && Model.BookNodeRoot.Base.buttoninfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.buttoninfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }
            if (Model.BookNodeRoot.Base.imageinfo != null && Model.BookNodeRoot.Base.imageinfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.imageinfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }
            if (Model.BookNodeRoot.Base.textinfo != null && Model.BookNodeRoot.Base.textinfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.textinfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }
            if (Model.BookNodeRoot.Base.modelinfo != null && Model.BookNodeRoot.Base.modelinfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.modelinfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }
            if (Model.BookNodeRoot.Base.camerainfo != null && Model.BookNodeRoot.Base.camerainfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.camerainfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }
            if (Model.BookNodeRoot.Base.audioinfo != null && Model.BookNodeRoot.Base.audioinfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.audioinfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }

            if (Model.BookNodeRoot.Base.clickinfo != null && Model.BookNodeRoot.Base.clickinfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.clickinfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.eventid,
                        Text = item.eventid

                    });
                }
            }
            if (Model.BookNodeRoot.Base.videoinfo != null && Model.BookNodeRoot.Base.videoinfo.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.videoinfo)
                {
                    Model.ResourceList.Add(new SelectListItem
                    {
                        Value = item.id,
                        Text = item.id

                    });
                }
            }


            if (Model.EventIdList == null)
            {
                Model.EventIdList = new List<SelectListItem>();
            }

            if (Model.BookNodeRoot.Base.openeventstate != null && Model.BookNodeRoot.Base.openeventstate.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.openeventstate)
                {
                    Model.EventIdList.Add(new SelectListItem
                    {
                        Value = item.enventid,
                        Text = item.name

                    });
                }
            }

            if (Model.BookNodeRoot.Base.closeeventstate != null && Model.BookNodeRoot.Base.closeeventstate.Count > 0)
            {
                foreach (var item in Model.BookNodeRoot.Base.closeeventstate)
                {

                    var resource = Model.EventIdList.Where(x => x.Value == item.enventid).ToList().FirstOrDefault();

                    if (resource == null)
                    {
                        Model.EventIdList.Add(new SelectListItem
                        {
                            Value = item.enventid,
                            Text = item.name
                        });
                    }
                    else
                    {
                        resource.Text = resource.Text + "&" + item.name;
                    }
                }
            }



        }
    }

}

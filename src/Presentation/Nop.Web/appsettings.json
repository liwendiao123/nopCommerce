    //Ios安装包地址
    "IosPackageUrl": "",
    // IOS 客户端版本
    "IosClientVersion": "",
    ///PC 客户端版本
    "PcClientVersion": "",
    ///PC 客户端地址
    "PcClientPackageUrl": "",
